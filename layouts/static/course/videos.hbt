{{> header css="/css/course.css" }}
{{> course/sidenav }}

<div id="content">
  <main>
  <div class="section hide-on-large-only" id="menu-banner"></div>
  <div class="row">
    <div class="col s12">
      {{#each lessons}}
      <div class="col s12 m6 l4">
        <a class="video-card" href="/{{ path }}/">
        <div class="card hoverable">
          <div class="card-image">
            <img src="//i.ytimg.com/vi/{{ firstVideo.youtube }}/maxresdefault.jpg"
            alt="YouTube placeholder">
            <span class="small flow-text card-title video-title">{{{ title }}}</span>
          </div>
        </div>
        </a>
      </div>
      {{/each}}
    </div>
  </div>
  </main>
</div>

{{> footer }}

